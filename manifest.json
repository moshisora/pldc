{
    "manifest_version": 2,
    "name": "pageLayoutDiffChecker",
    "description": "page layout diff checker",
    "version": "0.0.1",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon-48.png",
        "default_title": "pldc",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/jquery-3.1.1.min.js", "js/script.js"]
    }]
}
